{
    "name": "sergmoro1/yii2-uploader",
    "description": "File uploader. Upload multiple files at once. Fast Resize and Crop images in a queue. Any size available. Sorting by mouse. Control the max number of uploaded files. File description. Easy setup, quick start.",
    "keywords": [
        "yii2",
        "file", "image", "multiple",
        "upload", "crop", "resize", "thumb",
        "sorting", "description"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Morozov",
            "email": "sergmoro1@ya.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-imagine": "^2.2",
        "yiisoft/yii2-queue": "^2.3",
        "npm-asset/jquery-simple-upload" : "^1.1",
        "bower-asset/jcrop" : "^2.0"
    },
    "autoload": {
        "psr-4": {
            "sergmoro1\\uploader\\": "src"
        }
    }
}
